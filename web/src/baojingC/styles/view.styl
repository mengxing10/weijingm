.baojingcwrap
  // position: absolute;
  // top: 0
  // left: 0
  // bottom: 0
  // right: 0
  height: 100%
  background: #f6f6f6
  flexbox()
  flex-direction(column)
  .container
    margin-top: px2rem(30)
    // width: 100%
    // position: absolute;
    // top: 0
    // bottom px2rem(100)
    overflow-y: auto;
    padding 0 px2rem(20)
    box-sizing: border-box;
    background: #fff
    .item
      padding px2rem(32) 0
      clearfix()
      border-bottom 1px solid #f0f0f0
      .title
        float: left
        font-size px2rem(28)
        color #848689
      .con
        float: right
        font-size px2rem(29)
        color #000
    .item2
      padding px2rem(32) 0
      .title
        font-size px2rem(28)
        color #848689
      .con
        margin-top px2rem(40)
        margin-bottom px2rem(75)
        line-height: px2rem(40)
        font-size px2rem(29)
        color #000
  .chuliBtn
    width: 100%
    position: absolute;
    left: 0
    bottom: 0
    height: px2rem(86)
    line-height: px2rem(86)
    font-size: px2rem(34)
    clearfix()
    div
      float: left;
      width: 50%
      text-align: center
      color #fff
    div:first-child
      background-color: #646464
    div:last-child
      background-color: #64a3fb
