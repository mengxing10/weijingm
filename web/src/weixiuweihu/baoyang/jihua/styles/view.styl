.fix:after
    content: ""
    display: block
    height: 0
    clear: both
    visibility: hidden
.fix
    *zoom: 1;
.table-baoyangjihua
  width 100%
  background-size 100% 100%
  position: relative;
  overflow-y: auto;
  div
    font-size 16px
  .weixiu-title
    height 50px
    width 90%
    flexbox()
    margin 0 30px 0 20px
    justify-content(space-between)
    align-items(center)
    font-size 20px
    color #fff
    text-indent 10px
  .query-condition
    display flex
    margin-left 2%
    flex-align center
    align-items center
    flex-pack flex-start
    justify-content flex-start
    font-size 16px
    color #969696
    position relative
    margin-bottom 10px
    margin-top 10px
    padding-bottom 10px
    border-bottom 2px solid #7ac6ff
    label
        margin 0 10px
        color white
    .query-name
        font-size 16px
        margin-right 10px
        color white
    .query-value
        font-size 14px
        height 30px
        input
              border-radius 3px
        .ui.selection.dropdown
            min-height 30px
            padding-left 10px
            .dropdown.icon
                padding-right 5px
        .ui.dropdown
            color #fff
            background #3c75c2
            margin auto
            min-width  9em
            .default.text
                color #fff
            .ui.label
                color #fff
                background #0a448c
                font-weight normal
            .menu.visible
                color #fff
                background #3c75c2
                .item
                    color #fff
                    border-bottom 1px solid #5da6ea
                    &:hover
                        background #59a1e1
    .query-commit
        width 90px
        height 30px
        margin-left 10px
        line-height 30px
        text-align center
        background #254394
        color #fff
        border-radius 3px
        cursor pointer
        &:hover
            background #7ac6ff
        &.gray,
        &.gray:hover
            background #c6c6c6
    .query-btn
        width 90px
        height 30px
        line-height 30px
        text-align center
        background #254394
        color #fff
        border-radius 3px
        cursor pointer
        position absolute
        right 2%
        &:hover
            background #7ac6ff
        &.gray,
        &.gray:hover
            background #c6c6c6
    .dropdown
      min-width: 9em;
      min-height: 30px;
      line-height: 30px;
      padding-top: 0;
      padding-bottom: 0;
      .dropdown.icon
        width 20px
        right -38px
        top 7px
        margin 0
        padding 0
  .weixiu-table
    width 96%
    margin-left 2%
    .xinzeng-btn
      text-align: right
      margin-bottom: 10px
      span
        display: inline-block;
        cursor:pointer
        width: 90px
        height: 30px
        line-height: 30px
        text-align: center
        border-radius: 3px
        color: #fff
    .xinzeng-btn.xinzeng-btn2
      margin-top: 25px
      span
        width auto
        padding 0 10px
        margin-right: 20px
        background-color: #2a57ff;
        &:hover
          background-color: #2c8cf9;
  .modal-alert
     position: absolute;
     padding 10px
     width: 300px
     top 3%
     left: 50%
     transform translate(-50%,0)
     background-color: #fff
     border 1px solid #ccc
     border-radius: 5px
     box-shadow: 1px 2px 12px rgba(0,0,0,0.5)
     .title
       height: 30px
       line-height: 30px
       padding-left: 10px
     .content
       padding: 10px
       text-align: center
     .footer
       margin: 20px 0
       text-align: center;
       span
         cursor pointer
         display: inline-block;
         margin: 0 20px
         border: 1px solid #c6c6c6
         padding: 10px 15px
         border-radius: 5px
  .modal-alert.modal-guanlian
    width 60%
    .title
      height: auto;
      .spans
        float: right;
        text-align: right;
        .close_icon
          display: inline-block;
          width: 15px
          height: 15px
          margin-right 12px
          background: url(img/u9250.png) no-repeat
          background-size 100%
        &:hover
          .close_icon
            width: 16px
            height: 16px
        span
          cursor pointer
          display: inline-block;
          margin: 0 10px
          padding: 0 15px
          border-radius: 5px
          background: #0a5fbb
          color #fff
          font-size: 14px
    .content
       font-size: 14px
       max-height 400px
       overflow: auto
       li
         padding: 10px 0
         border-top 1px solid #ccc
         span
           float left
           width 28%
           text-align: left
         .li_span1
           width 12%
           text-align: center
       li:last-child
         border-bottom 1px solid #ccc
  .modal-alert.modal-timemodel.modal-jihua
     width 60%
     .content
       max-height: 100%
  .modal-jihua
     .item_lists
       padding: 10px 0
       line-height: 1.5
     .div_w_4.div_w_label
       width: 20%
       margin-right: 15px
       text-align: right;
     .div_w_4
       float: left
       width: 25%
       text-align: left
     .div_w_4_3
       float left
       width: 75%
       text-align: left;
  .modal-alert.modal-timemodel.modal-jihua.modal-jihua-bg
     background-color: #349aef
     color #f6f6f6
     .content
       margin-bottom 30px
     .content_title
       text-align: center;
       color #f6f6f6
       padding-bottom 10px
       font-size 20px
     .content_tips
       padding: 20px
       color #f6f6f6
       text-align: left
       font-size: 14px
     .content_search
       text-align left
       margin-bottom 20px
       .commitBtn
         padding: 6px 15px
         background-color: #3c75c2
         vertical-align: middle
         &:hover
           background-color: #1e4386
