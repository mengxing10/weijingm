$textColor= #d7eafb

.theheader 
     background-color #2f2e4b
     box-shadow 0px 6px 30px 9px rgba(19, 20, 32, 0.1)
     h1 p span 
            color #e4e4fc!important
     .theMenu
            background-color #646390
     .usericon
        background url('./img/gray_user.png') no-repeat center center
.theBody
    background-color #25253d 


    .contentDiv
               background-color #2f2e4b
               border-radius 3px
               padding 20px 0px
    .contentDiv2
               background-color #2f2e4b
               border-radius 3px   
    .contentDiv3
               background-color #2f2e4b      
    .tabBkColor1
               background-color #333254!important
    .tabBkColor2
               background-color #333254!important  /* old:#6597f5003466 */
               .div1_1 .div1_1_left
                  background-color #646390!important  /* old: #0049f9  按钮颜色*/
               .abottom
                  color #a2a1b3!important /* old: ##6597f5  第二行文本颜色*/
    .labStyle /* 2018.5.2  label text color :  white:#859db4  gray #a2a1b3   blue: #a3cff6 */
               //font-size 15px
               color #a2a1b3!important    /*gray: #859db4  blue: #d7eafb  */
               .numStyle
                   color #ffffff
    .gao-head  
              background-color #77a3fe
    .liang-head
              background-color #3cbdc6              
    .zhong-head
              background-color #8d59f8
    .di-head
              background-color #ff7687                  
    .commitBtn,.rightBtn
                width 90px
                height 30px
                margin-left 10px
                line-height 30px
                text-align center
                background #646390
                color #fff!important
                border-radius 3px
                cursor pointer
                &:hover
                    background  #3e3c90  
    .rightBtn
            position absolute
            right 2%
    .el-mytable
        .el-tbody 
            &:nth-child(even)
                background #333254  /* blue: #20529e gray: #333254 #f9fafc #1f3d81*/
        .el-thead,.el-tbody
             background  #2f2e4b /*white #152c6c  blue: #1d468a */
             li
                color #ffffff   /*  white: #405e7c*/
.mainfooter
  background-color #2f2e4b!important
  color #e4e4fc!important
  
menuBgColor = #1e202f  /* white:#ffffff,  gray: #1e202f  blue: #14204e */
menuItemColor = #9e9eab /* white:#b2c2d0,  gray: #9e9eab blue: #a3cff6 */
menuItemOverColor = #646390 /*white:#405e7c    gray: #646390 blue: #2c8cf9 */
menuItemActiveColor = #ffffff /*white:#4886ff    gray: #ffffff blue: #ffffff*/
   

.charts-menu-2
     background  menuBgColor
     .charts-menu-list       
         .charts-menu-item
             color menuItemColor       
             &:hover
                 color menuItemOverColor             
             &.active
                 color menuItemActiveColor            
             span          
                 dd                 
                     color menuItemColor                  
          .charts-menu-item-child
              background menuBgColor             
              div
                  color menuItemColor                
                  &.active                
                      color menuItemActiveColor    
                  &:hover                
                      color menuItemOverColor

.per-content
     .charts-menu-list2 
          background menuBgColor    
          .charts-menu-item  
                color menuItemColor           
          .charts-menu-item.active           
                color menuItemActiveColor           
          .charts-menu-item:hover  
                color menuItemOverColor            
     .right-content   
         .right-menus           
             background menuBgColor /* #0f1d52 */
             .mitem            
                 color menuItemColor /* #7598f7 */               
             .mitem.active            
                color menuItemActiveColor /* #fff */               
             .mitem:hover  
                color menuItemOverColor /* #fff */   

         .el-edittable
              .el-table
                  .el-tbody 
                      &:nth-child(even)
                          background #333254      /* blue: #20529e gray: #333254  white: white */
                  .el-thead,.el-tbody
                       border 0px solid #3f6cbb
                       background  #2f2e4b /*white #f9fafc  blue: #1d468a */
                       li
                          color #ffffff   /*  white: #405e7c blue: #ffffff */  
                          border 0px solid #3f6cbb        